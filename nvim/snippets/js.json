{
	"console.log": {
		"prefix": "cl",
		"body": [
      "console.log($1)"
		]
	},
	"console.log json stringify": {
		"prefix": "clj",
		"body": [
      "console.log(JSON.stringify($1, null, 2))"
		]
	},
  "eslint explicit any": {
    "prefix": "explicitAny",
    "body": "// eslint-disable-next-line @typescript-eslint/no-explicit-any"
  },
	"jest test": {
		"prefix": "it",
		"body": [
      "it(\"$1\", () => {",
      "  // given",
      "",
      "  // when",
      "  $2",
      "",
      "  // then",
      "})"
		]
	},
	"jest describe": {
		"prefix": "describe",
		"body": [
      "describe(\"$1\", () => {",
      "  it(\"$2\", () => {",
      "    // given",
      "",
      "    // when",
      "    $3",
      "",
      "    // then",
      "  })",
      "})"
		]
	}
}
